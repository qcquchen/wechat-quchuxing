<view hidden="{{ !homeOfwork }}" style="width: {{ video_width }}px; height:{{ video_height }}px;" >	
	<map id="map" 
		longitude="{{ longitude }}" 
		latitude="{{ latitude }}" 
		scale="16" 
		controls="{{ controls }}" 
		bindcontroltap="controltap" 
		markers="{{ markers }}" 
		bindregionchange="regionchange"
		bindmarkertap="markertap" 
		show-location 
		style="width: 100%; height: 100%;"
		></map>
</view>
<view hidden="{{ homeOfwork }}" style="width: {{ video_width }}px; height:{{ video_height - 257 }}px;" >	
	<map id="map" 
		longitude="{{ longitude }}" 
		latitude="{{ latitude }}" 
		scale="16" 
		markers="{{ samll_markers }}" 
		bindcontroltap="controltap" 
		bindmarkertap="markertap" 
		polyline="{{ polyline }}" 
		controls="{{ controls_samll }}"
		style="width: 100%; height: 100%;"
		bindtap="hideCode"
		></map>
</view>
<view class="form_journey" animation="{{ !homeOfwork ? selectJourney_animation : '' }}">
	<view hidden="{{ hideOfShow_type != 'form' }}">
		<view class="select_identity">
			<view class="{{ switch_identity == 'passenger' ? 'select_identity_active' : ''}}" bindtap="passenger">乘客</view>
			<view class="{{ switch_identity == 'Owners' ? 'select_identity_active' : ''}}" bindtap="owners">车主</view>
		</view>
		<view class="select_multiSelect">
			<picker mode="multiSelector" bindchange="bindMultiPickerChange" bindcolumnchange="bindMultiPickerColumnChange" value="{{timeIndex}}" range="{{timeArray}}">
				<view class="select_tiem">
						<view class="icon">
							<image src="../images/btn_card_time@3x.png"></image>
						</view>
						<view class="common_select">
							<view>出发时间</view>
						    	<view class="time">
							      {{timeArray[0][timeIndex[0]]}}{{timeArray[1][timeIndex[1]]}}{{timeArray[2][timeIndex[2]]}}
							    </view>
						</view>
				</view>
		    </picker>
			<picker bindchange="bindPickerChange_Seats" value="{{seat_number_index}}" range="{{seat_number}}">
				<view class="select_tiem select_tiem_noBorder">
					<view class="icon">
						<image src="../images/btn_card_seat@3x.png"></image>
					</view>
					<view class="common_select">
						<block wx:if="{{ switch_identity == 'passenger' }}">
							<view>乘车人数</view>
						</block>
						<block wx:else>
							<view>座位数量</view>
						</block>
				    	<view class="time">
				      	{{seat_number[seat_number_index]}}
				    	</view>
					</view>
				</view>
		    </picker>
		    <block wx:if="{{ switch_identity != 'passenger' }}">
				<view class="select_tiem" bindtap="selectLine">
					<view class="icon">
						<image src="../images/btn_card_line@3x.png"></image>
					</view>
					<view class="common_select">
						<view>选择路线</view>
				    	<view class="time">
				    		{{ strategy_active }}
				    	</view>
					</view>
				</view>
				<picker bindchange="bindPickerChange_Price" value="{{select_price_index}}" range="{{select_price}}">
					<view class="select_tiem select_tiem_noBorder">
						<view class="icon">
							<image src="../images/btn_card_money@3x.png"></image>
						</view>
						<view class="common_select">
							<view>票价 (元 / 位)</view>
						    	<view class="time">
						      	{{select_price[select_price_index]}} 元/位
						    	</view>
						</view>
					</view>
			    </picker>
		    </block>
		    <block wx:if="{{ btn_hideOfShow == 'hide' }}">
				<view class="commit_journey">
					<view class="call_off" bindtap="hideCode">稍后</view>
					<view class="affirm" bindtap="commit_journey">确认</view>
				</view>
			</block>
		</view>
	</view>
	<view style="position: relative;" hidden="{{ hideOfShow_type != 'info' }}">
		<view class="code_journey_info">
			<view class="code_journey_title">15分钟后自动确认上车<text>延时</text></view>
			<view class="navigation">
				<view class="navigation_up">
					<view class="nav_img">
						<image></image>
					</view>
					<view class="nav_info">
						<view class="nav_info_news">导航至上车点</view>
						<view class="nav_loaction">工贸技师学院</view>
					</view>
				</view>
				<view class="navigation_down">
					<view class="nav_img">
						<image></image>
					</view>
					<view class="nav_info">
						<view class="nav_info_news">导航至上车点</view>
						<view class="nav_loaction">工贸技师学院</view>
					</view>
				</view>
			</view>
			<view class="car_master">
				<view class="car_master_info">
					<view class="car_img">
						<image src="../images/img_me.jpg"></image>
					</view>
					<view class="car_news">
						<view class="car_master_name">王晓明<text>10分钟前</text></view>
						<view class="car_master_code"><text>京A54563</text> 奥迪 银色</view>
					</view>
				</view>
				<view class="car_timeAndPhone">
					<view>08:30</view>
					<view>电话</view>
				</view>
			</view>
			<view class="car_passenger">
				<view class="car_passenger_img">
					<image src="../images/img_me.jpg"></image>
				</view>
				<view class="car_passenger_img">
					<image src="../images/img_me.jpg"></image>
				</view>
				<view class="car_passenger_img">
					<image src="../images/img_me.jpg"></image>
				</view>
				<view class="car_passenger_img">
					<image src="../images/img_me.jpg"></image>
				</view>
			</view>
		</view>
		<view class="car_foot_btn">
			<view class="baoqilai">
				<view>客服</view>
				<view>车主迟到</view>
				<view>取消行程</view>
				<view>分享</view>
			</view>
		</view>
	</view>
</view>

<swiper indicator-dots="true" bindchange="durationChange" hidden="{{ !homeOfwork }}">
	<swiper-item>
		<view class="tab_index">
			<view class="index_common selct_work_home">
				<view class="select" data-id="2" data-type="form" bindtap="goHomeOfWork">
					<image src="../images/icon_home@3x.png"></image>
					<view>回家</view>
				</view>
				<view>|</view>
				<view class="select" data-id="1" data-type="form" bindtap="goHomeOfWork">
					<image src="../images/icon_company@3x.png"></image>
					<view>公司</view>
				</view>
			</view>
		</view>
	</swiper-item>
	<block wx:for="{{imgUrls}}" wx:key="{{index}}">
		<swiper-item>
			<view class="tab_index" data-type="info" bindtap="goHomeOfWork">
				<view class="journey_list index_common">
					<view class="journey_img">
						<image src="../images/img_me.jpg"></image>
					</view>
					<view class="journey_news">
						<view class="journey_title">明天10:30记得准时上车哦agcasjk</view>
						<view class="journey_info"><text class="car_name">王晓明 </text><text class="car_code">京A88888</text> 奥迪A6L 白金色lalalalla</view>
					</view>
				</view>
			</view>
		</swiper-item>
	</block>
</swiper>
